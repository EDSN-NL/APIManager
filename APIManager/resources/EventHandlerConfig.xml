           <HandlerFQN>Plugin.Application.Events.CodeList.CodeListMajorVersionEvent</HandlerFQN>
           <StereotypeList>
             <!-- This event only works on packages that have the 'CodeListDeclaration' stereotype. -->
             <StereotypeName>Enexis ECDM::CodeListDeclaration</StereotypeName>
           </StereotypeList>
         </EventHandler>
       </EventHandlerList>
     </EventGroup>
     <EventGroup ID="REST Services">
       <EventHandlerList>
         <EventHandler ID="Interface: Process">
           <HandlerFQN>Plugin.Application.Events.API.ProcessRESTInterfaceEvent</HandlerFQN>
           <StereotypeList>
