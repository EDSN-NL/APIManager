             <StereotypeName>Enexis ECDM::ServiceContainer</StereotypeName>
           </StereotypeList>
         </EventHandler>
         <EventHandler ID="Delete Code List">
           <HandlerFQN>Plugin.Application.Events.CodeList.DeleteCodeListEvent</HandlerFQN>
           <StereotypeList>
             <!-- This event only works on a class that has the 'CodeList' stereotype. -->
             <!-- We don't have to check package context since CodeLists can only exist in a single, correct, package type. -->
             <StereotypeName>Enexis ECDM::CodeList</StereotypeName>
           </StereotypeList>
         </EventHandler>
         <EventHandler ID="Process Code List">
           <HandlerFQN>Plugin.Application.Events.CodeList.ProcessCodeListEvent</HandlerFQN>
           <StereotypeList>
             <!-- This event only works on a class that has the 'CodeList' stereotype. -->
